<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Byte Client" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="..\..\bin\Byte" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\bin\" />
				<Option object_output="..\..\obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-std=c99" />
					<Add option="-ggdb3" />
					<Add option="-DGLEW_STATIC" />
					<Add directory="..\..\ext\freetype" />
					<Add directory="..\..\ext" />
					<Add directory="..\..\ext\json" />
				</Compiler>
				<Linker>
					<Add option="-lglfwWIN" />
					<Add option="-lsoilWIN" />
					<Add option="-lfreetypeWIN" />
					<Add option="-lzWIN" />
					<Add option="-lopengl32" />
					<Add option="-static-libgcc" />
					<Add directory="..\..\ext" />
				</Linker>
				<MakeCommands>
					<Build command="mingw32-make.exe -f $makefile Byte" />
					<CompileFile command="mingw32-make.exe -f $makefile $file" />
					<Clean command="mingw32-make.exe -f $makefile cleanByte" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="mingw32-make.exe -q -f $makefile Byte" />
					<SilentBuild command="mingw32-make.exe -s -f $makefile Byte" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\Byte" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
